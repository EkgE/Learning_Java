public class Bed {
    private String size;
    private String type;
    private int persons;
    private BedSheets bedSheets;

    public Bed(String size, String type, int persons, BedSheets bedSheets){
        this.size = size;
        this.type = type;
        this.persons = persons;
        this.bedSheets = bedSheets;
    }
    public String getSize() {
        return size;
    }
    public String getType() {
        return type;
    }
    public int getPersons() {
        return persons;
    }
    public BedSheets getBedSheets(){
        return bedSheets;
    }
}
