public class NightStand {
    private String type;
    private int age;
    private Lamp lamp;



    public NightStand(String type, int age, Lamp lamp) {
        this.type = type;
        this.age = age;
        this.lamp = lamp;
    }
    public Lamp getLamp() {
        return lamp;
    }

    public String getType() {
        return type;
    }

    public int getAge() {
        return age;
    }

    public NightStand(String type, int age) {
        this.type = type;
        this.age = age;
    }
}
