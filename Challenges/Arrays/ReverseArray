class ReverseArray {
    public static void main(String[] args){


    }
     private static void reverse(int[] array){
        int temp;
        for(int i = 0; i < array.length; i++){
            for(int j = i + 1; j < array.length; j++){
                temp = array[i];
                array[i] = array[j];
                array[j] = temp;
            }
        }
    }
}
